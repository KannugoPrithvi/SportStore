@model IEnumerable<string>


<li class="active">
    @Html.ActionLink("Home", "List", new { controller = "Product" })
</li>


@foreach (var link in Model)
{
    <li>
        @Html.RouteLink(link, new
            {
                controller = "Product",
                action = "List",
                category = link,
                page = 1
            })
    </li>
}

