@model IEnumerable<string>


<li>
    @Html.ActionLink("Home", "List", new { controller = "Product" }) <span class='badge'>24</span>   
</li>


@foreach (var link in Model)
{
    <li>
        @Html.RouteLink(link, new
            {
                controller = "Product",
                action = "List",
                category = link,
                page = 1
            })
        <span class='badge'>24</span> 
    </li>
}

